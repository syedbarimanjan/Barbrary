<h2>search books</h2>
<form action="/books" method="GET">
    <div>
        <label for="title">Title</label>
        <input type="text" name="title" value="<%if(locals.searchParams != null){%><%=searchParams.title%><%}%>">
    </div>
    <div>
        <label for="publishedAfter">published after</label>
        <input type="date" name="publishedAfter" value="<%if(locals.searchParams != null){%><%=searchParams.publishedAfter%><%}%>">
    </div>
    <div>
        <label for="publishedBefore">published before</label>
        <input type="date" name="publishedBefore" value="<%if(locals.searchParams != null){%><%=searchParams.publishedBefore%><%}%>">
    </div>
    <button type="submit">Serach</button>
</form>
<br>
<div>
    <% books.forEach(book => { %>
        <img src="<%= book.coverImagePath %>" alt="" height="150" width="100">
    <% }) %>
</div>